{
  "name": "polythene-mithril-coffeescript-setup",
  "description": "Polythene for Mithril with CoffeeScript setup example",
  "version": "1.6.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/polythene-mithril-coffeescript-setup.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "webpack-dev-server --config scripts/webpack.config.dev.js --disableHostCheck true --port 3000",
    "dev:watch": "webpack --watch --config scripts/webpack.config.dev.js",
    "build": "webpack --config scripts/webpack.config.prod.js",
    "serve": "http-server -p 5000 dist"
  },
  "dependencies": {
    "mithril": "^2.0.0-rc.6",
    "polythene-css": "1.6.2",
    "polythene-mithril": "1.6.2"
  },
  "devDependencies": {
    "coffee-loader": "latest",
    "coffeescript": "latest",
    "compression-webpack-plugin": "3.0.0",
    "css-loader": "2.1.1",
    "mini-css-extract-plugin": "0.7.0",
    "npm-run-all": "4.1.5",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.3",
    "webpack-dev-server": "3.7.1"
  }
}
